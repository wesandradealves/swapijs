{
  "name": "swapi-js",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://swapi-js-2.herokuapp.com/",
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-lazy": "^1.1.0-beta.0",
    "react-loader-spinner": "^2.3.0",
    "react-parallax-mousemove": "^1.1.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "sass": "^1.20.1",
    "sass-rem": "^2.0.1",
    "shuffle-array": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "server": "cd api && node server",
    "client": "cd src && npm start",
